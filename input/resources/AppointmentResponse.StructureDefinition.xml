<?xml version="1.0" encoding="utf-8"?>
<StructureDefinition xmlns="http://hl7.org/fhir">
  <id value="AppointmentResponse" />
  <url value="https://ancode.app/fhir/StructureDefinition/AppointmentResponse" />
  <version value="1.0" />
  <name value="AppointmentResponse" />
  <title value="Resposta do Compromisso" />
  <status value="draft" />
  <description value="Uma resposta a uma solicitação de compromisso para um paciente e/ou médico(s), como uma confirmação ou rejeição." />
  <fhirVersion value="4.0.1" />
  <kind value="resource" />
  <abstract value="false" />
  <type value="AppointmentResponse" />
  <baseDefinition value="http://hl7.org/fhir/StructureDefinition/AppointmentResponse" />
  <derivation value="constraint" />
  <differential>
    <element id="AppointmentResponse">
      <path value="AppointmentResponse" />
      <short value="Resposta do Compromisso" />
      <definition value="Uma resposta a uma solicitação de compromisso para um paciente e/ou médico(s), como uma confirmação ou rejeição." />
    </element>
    <element id="AppointmentResponse.identifier">
      <path value="AppointmentResponse.identifier" />
      <short value="Identificações" />
      <definition value="Identificadores associados a essa questão de resposta ao compromisso." />
      <type>
        <code value="Identifier" />
        <profile value="https://ancode.app/fhir/StructureDefinition/Identifier" />
      </type>
    </element>
    <element id="AppointmentResponse.appointment">
      <path value="AppointmentResponse.appointment" />
      <short value="Compromisso" />
      <definition value="Compromisso ao qual esta resposta está referenciada." />
    </element>
    <element id="AppointmentResponse.start">
      <path value="AppointmentResponse.start" />
      <short value="Horário Inicial" />
      <definition value="Data/hora em que o compromisso deve ocorrer ou nova hora de início solicitada." />
      <min value="1" />
    </element>
    <element id="AppointmentResponse.end">
      <path value="AppointmentResponse.end" />
      <short value="Horário Final" />
      <definition value="Pode ser o mesmo horário final do compromisso ou, alternativamente, um novo horário para solicitar uma renegociação do horário de término." />
      <min value="1" />
    </element>
    <element id="AppointmentResponse.participantType">
      <path value="AppointmentResponse.participantType" />
      <short value="Função do Participante" />
      <definition value="Função do Participante no compromisso." />
      <min value="1" />
    </element>
    <element id="AppointmentResponse.actor">
      <path value="AppointmentResponse.actor" />
      <short value="Uma Pessoa, Local, Serviço de Saúde ou Dispositivo" />
      <definition value="Uma Pessoa, Local, Serviço de Saúde ou Dispositivo que está participando do compromisso." />
      <min value="1" />
    </element>
    <element id="AppointmentResponse.participantStatus">
      <path value="AppointmentResponse.participantStatus" />
      <short value="Status" />
      <definition value="Status da participação. Quando o status é recusado ou provisório, se os horários de início/término forem diferentes do compromisso, esses horários devem ser interpretados como uma alteração de horário solicitada. Quando o status é aceito, os horários podem ser o horário do compromisso (como uma confirmação do horário)." />
    </element>
    <element id="AppointmentResponse.comment">
      <path value="AppointmentResponse.comment" />
      <short value="Comentários Adicionais" />
      <definition value="Comentários adicionais sobre o compromisso." />
    </element>
  </differential>
</StructureDefinition>